{
    "project":"simulate_isim",
    "installed tools": [
        "xilinx",
        "altera",
        "icarus"
    ],
    "flow_steps": {
	"1" : "clean",
	"2" : "xvlog",
    "3" : "xelab",
    "4" : "xsim"
    },
    "flow" : {
	"clean" :{
	    "executable" : "rm",
	    "arguments" : "-f *.log *.pb *~ webtalk* xsim* "
	},
	"xvlog" :{
	    "executable" : "xvlog",
        "arguments" : " -f rtl_sim.f"
	},
    "xelab" :{
        "executable":"xelab",
        "arguments" : "work.glbl work.testbench -L unisims_ver -L unimacro_ver -L xilinxcorelib_ver -prj rtl_sim.f"
    },
	"xsim" : {
	    "executable": "xsim",
	    "arguments" : " work.glbl#work.testbench"
	},
	"run" : {
	    "executable" : "./a.out",
	    "arguments"  : " "
	}
    }
}
